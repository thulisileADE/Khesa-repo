<ivysettings>
  <settings defaultResolver="ibiblio"/>
  <resolvers>
    <chain name="apache-hadoop" dual="true">
      <ibiblio name="apache-snapshot" root="https://repository.apache.org/content/repositories/snapshots/" m2compatible="true" />
      <ibiblio name="ibiblio" m2compatible="false" />
    </chain>
  </resolvers>
  <modules>
     <module organisation="org.apache.hadoop" name="*" resolver="apache-hadoop"/>
  </modules>
</ivysettings>
